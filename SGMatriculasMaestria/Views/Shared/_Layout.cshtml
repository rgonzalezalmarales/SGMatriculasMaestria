<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewData["Title"] - SGMAI</title>

    <!-- Custom fonts for this template-->
    <link href="~/lib/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">

    <!-- Custom styles for this template-->
    <link href="~/css/sb-admin-2.min.css" rel="stylesheet">
    <link href="~/css/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://unpkg.com/aos@next/dist/aos.css" />

</head>
<body class="hold-transition sidebar-mini sidebar-collapse layout-fixed layout-navbar-fixed layout-footer-fixed">

    <!-- Page Wrapper -->
    <div id="wrapper">
        <!-- Sidebar -->
        <ul class="navbar-nav bg-info sidebar sidebar-dark accordion" id="accordionSidebar" style="animation-fill-mode">
            <!-- Sidebar - Brand -->
            <li>
                <a class="sidebar-brand d-flex align-items-center justify-content-center" href="/">
                    <div class="sidebar-brand-icon">
                        <i class="fas fa-university"></i>
                    </div>
                    <div class="sidebar-brand-text mx-3">SGMAI</div>
                </a>
            </li>
            <!-- Divider -->
            <li><hr class="sidebar-divider" /></li>
            <!-- Nav Item - Pages Collapse Menu -->
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#aspirantes"
                   aria-expanded="true" aria-controls="aspirantes">
                    <i class="fas fa-fw fa-users"></i>
                    <span>Aspirantes</span>
                </a>
                <div id="aspirantes" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <a class="collapse-item" asp-area="" asp-controller="Aspirantes" asp-action="Index">
                            <span>Lista de Aspirantes</span>
                        </a>
                        <a class="collapse-item" asp-area="" asp-controller="Aspirantes" asp-action="Create">
                            <span>Crear Aspirante</span>
                        </a>
                    </div>
                </div>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#maestrias"
                   aria-expanded="true" aria-controls="maestrias">
                    <i class="fas fa-fw fa-file-word"></i>
                    <span>Mestrías</span>
                </a>
                <div id="maestrias" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <a class="collapse-item" asp-area="" asp-controller="Maestrias" asp-action="Index">
                            <span>Lista de Maestrías</span>
                        </a>
                        <a class="collapse-item" asp-area="" asp-controller="Maestrias" asp-action="Create">
                            <span>Crear Maestría</span>
                        </a>
                    </div>
                </div>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#matriculas"
                   aria-expanded="true" aria-controls="matriculas">
                    <i class="fas fa-fw fa-user-edit"></i>
                    <span>Matrículas</span>
                </a>
                <div id="matriculas" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <a class="collapse-item" asp-area="" asp-controller="Matriculas" asp-action="Index">
                            <span>Lista de Matrículas</span>
                        </a>
                        <a class="collapse-item" asp-area="" asp-controller="Matriculas" asp-action="Create">
                            <span>Crear Matrícula</span>
                        </a>
                    </div>
                </div>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#especgraduadoes"
                   aria-expanded="true" aria-controls="especgraduadoes">
                    <i class="fas fa-fw fa-clipboard-list"></i>
                    <span>Especialidades</span>
                </a>
                <div id="especgraduadoes" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <a class="collapse-item" asp-area="" asp-controller="EspecGraduadoes" asp-action="Index">
                            <span>Lista de Especialidades</span>
                        </a>
                        <a class="collapse-item" asp-area="" asp-controller="EspecGraduadoes" asp-action="Create">
                            <span>Crear Especialidad</span>
                        </a>
                    </div>
                </div>
            </li>
            <!-- Nav Item - Pages Collapse Menu -->
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapsePages"
                   aria-expanded="true" aria-controls="collapsePages">
                    <i class="fas fa-university"></i>
                    <span>Universidades</span>
                </a>
                <div id="collapsePages" class="collapse" aria-labelledby="headingPages" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <a class="collapse-item" asp-area="" asp-controller="Ces" asp-action="Index">
                            <span>Listar Universidades</span>
                        </a>
                        <a class="collapse-item" asp-area="" asp-controller="Ces" asp-action="Create">
                            <span>Crear Universidad</span>
                        </a>
                        <div class="collapse-divider">
                            <hr class="sidebar-divider"/>
                        </div>
                        <a class="collapse-item" asp-area="" asp-controller="Facultades" asp-action="Index">
                            <span>Listar Facultades</span>
                        </a>
                        <a class="collapse-item" asp-area="" asp-controller="Facultades" asp-action="Create">
                            <span>Crear Facultad</span>
                        </a>

                    </div>
                </div>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#centrotrabajoes"
                   aria-expanded="true" aria-controls="centrotrabajoes">
                    <i class="fas fa-fw fa-folder"></i>
                    <span>Centros de Trabajo</span>
                </a>
                <div id="centrotrabajoes" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <a class="collapse-item" asp-area="" asp-controller="CentroTrabajoes" asp-action="Index">
                            <span>Lista de Centros de Trabajo</span>
                        </a>
                        <a class="collapse-item" asp-area="" asp-controller="CentroTrabajoes" asp-action="Create">
                            <span>Crear Centro de Trabajo</span>
                        </a>
                    </div>
                </div>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#secretariopostgs"
                   aria-expanded="true" aria-controls="secretariopostgs">
                    <i class="fas fa-fw fa-calendar"></i>
                    <span>Secretarios Post-Grado</span>
                </a>
                <div id="secretariopostgs" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <a class="collapse-item" asp-area="" asp-controller="SecretarioPostgs" asp-action="Index">
                            <span>Lista de Secretarios</span>
                        </a>
                        <a class="collapse-item" asp-area="" asp-controller="SecretarioPostgs" asp-action="Create">
                            <span>Crear Secretario</span>
                        </a>
                    </div>
                </div>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#categdocentes"
                   aria-expanded="true" aria-controls="categdocentes">
                    <i class="fas fa-fw fa-flag"></i>
                    <span>Categorias Docentes</span>
                </a>
                <div id="categdocentes" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <a class="collapse-item" asp-area="" asp-controller="CategDocentes" asp-action="Index">
                            <span>Lista de Categorias</span>
                        </a>
                        <a class="collapse-item" asp-area="" asp-controller="CategDocentes" asp-action="Create">
                            <span>Crear Categoria</span>
                        </a>
                    </div>
                </div>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#paises"
                   aria-expanded="true" aria-controls="paises">
                    <i class="fas fa-globe-americas"></i>
                    <span>Ubicaciones</span>
                </a>
                <!-- Nav Item - Charts -->
                <div id="paises" class="collapse" aria-labelledby="headingPages" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <a class="collapse-item" asp-area="" asp-controller="Pais" asp-action="Index">
                            <span>Listar Paises</span>
                        </a>
                        <a class="collapse-item" asp-area="" asp-controller="Pais" asp-action="Create">
                            <span>Crear Pais</span>
                        </a>
                        <div class="collapse-divider"></div>
                        <a class="collapse-item" asp-area="" asp-controller="Provincias" asp-action="Index">
                            <span>Listar Provincias</span>
                        </a>
                        <a class="collapse-item" asp-area="" asp-controller="Provincias" asp-action="Create">
                            <span>Crear Provincia</span>
                        </a>

                        <div class="collapse-divider"></div>
                        <a class="collapse-item" asp-area="" asp-controller="Municipios" asp-action="Index">
                            <span>Listar Municipios</span>
                        </a>
                        <a class="collapse-item" asp-area="" asp-controller="Municipios" asp-action="Create">
                            <span>Crear Municipio</span>
                        </a>
                    </div>
                </div>
            </li>
            <!-- Divider -->
            <hr class="sidebar-divider d-none d-md-block">
            <!-- Sidebar Toggler (Sidebar) -->
            <div class="text-center d-none d-md-inline">
                <button class="rounded-circle border-0" id="sidebarToggle"></button>
            </div>
        </ul>
        <!-- End of Sidebar -->
        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">
            <!-- Main Content -->
            <div id="content">
                <!-- Topbar -->
                <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">
                    <!-- Sidebar Toggle (Topbar) -->
                    <button id="sidebarToggleTop" class="btn btn-link d-md-none rounded-circle mr-3">
                        <i class="fa fa-bars"></i>
                    </button>
                    <!-- Topbar Navbar -->
                    <ul class="navbar-nav ml-auto">
                        <!-- Nav Item - Search Dropdown (Visible Only XS) -->
                        <!-- Dropdown - Messages -->
                        <div class="dropdown-menu dropdown-menu-right p-3 shadow animated--grow-in">
                        </div>
                        <div class="topbar-divider d-none d-sm-block"></div>
                        <!-- Nav Item - User Information -->
                        <li class="nav-item dropdown no-arrow">
                            <partial name="_LoginPartial" />
                            <!-- Dropdown - User Information -->
                        </li>
                    </ul>
                </nav>
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>
        </div>
        <!-- End of Content Wrapper -->
    </div>
    <!-- End of Page Wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Logout Modal-->
    <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
         aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
                    <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
                <div class="modal-footer">
                    <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
                    <a class="btn btn-info" href="login.html">Logout</a>
                </div>
            </div>
        </div>
    </div>

    <footer class="sticky-footer bg-i">
        <div class="container my-auto">
            <div class="copyright text-center my-auto">
                <span>SGMAI - Sistema de Gestion de Matriculas en Maestrias y Especialidades &copy; </span>
            </div>
        </div>
    </footer>

    <!-- Bootstrap core JavaScript-->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="~/lib/jquery-easing/jquery.easing.min.js"></script>


    <!-- Custom scripts for all pages-->
    <script src="~/js/sb-admin-2.min.js"></script>

    <!-- Page level plugins -->
    <script src="~/js/chart.js/Chart.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="~/js/demo/chart-area-demo.js"></script>
    <script src="~/css/datatables/jquery.dataTables.min.js"></script>
    <script src="~/css/datatables/dataTables.bootstrap4.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/Matricula.js" asp-append-version="true"></script>
    <script src="https://unpkg.com/aos@next/dist/aos.js"></script>
    <script src="https://unpkg.com/aos@next/dist/aos.js"></script>
    <script>
        AOS.init();
    </script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
